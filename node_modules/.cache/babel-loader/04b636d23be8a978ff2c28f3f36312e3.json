,uCAAA,EAAA,CAAA,GAAA,GAAA,UAAA,GAAA,EAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,aAAA;AACA;AACA,OANA;AAOA,KApCA;AAqCA,IAAA,OArCA,qBAqCA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,SAAA,EAAA,KAAA,KAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,yCAAA,KAAA,SAAA,GAAA,UAAA,GAAA,KAAA,KAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,GAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA;;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,aAAA;AACA;AACA,OAPA;AAQA,KA/CA;AAgDA,IAAA,aAhDA,yBAgDA,IAhDA,EAgDA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,sCAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,GAAA,GAAA,GAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,eAAA,GAAA,IAAA;AACA,OAHA;AAIA;AArDA,GA3BA;AAkFA,EAAA,OAlFA,qBAkFA;AACA,SAAA,SAAA;AACA;AApFA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item>首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>订单管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>订单列表</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n     <!-- 卡片视图区域 -->\r\n    <el-card>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"5\">\r\n          <el-input placeholder=\"请输入姓名\" v-model=\"queryInfo.query\" clearable></el-input>\r\n        </el-col>\r\n        <el-col :span=\"3\">\r\n          <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"search\">搜索</el-button>\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 订单列表区 -->\r\n      <el-table  v-loading=\"loading\" :data=\"orders\" border stripe>\r\n        <el-table-column type=\"index\"></el-table-column>\r\n        <el-table-column label=\"订单餐号\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            {{scope.row.wait}}\r\n          </template>\r\n        </el-table-column>\r\n         <el-table-column label=\"购买人\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n            {{scope.row.user.nike}}\r\n          </template>\r\n         </el-table-column>\r\n          <el-table-column label=\"购买人电话\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n            {{scope.row.user.phone}}\r\n          </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"订餐状态\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tag v-if=\"scope.row.wstate==0\" type=\"warning\">备餐中</el-tag>\r\n              <el-tag v-if=\"scope.row.wstate==1\" type=\"danger\">待取餐</el-tag>\r\n              <el-tag v-if=\"scope.row.wstate==2\" type=\"success\">已完成</el-tag>\r\n          </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"购买时间\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n            {{scope.row.otime | timeflow}}\r\n          </template>\r\n         </el-table-column>\r\n        <el-table-column label=\"操作\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              @click=\"toEdit(scope.row.wait,scope.row.wstate)\"\r\n              type=\"primary\"\r\n              icon=\"el-icon-edit\"\r\n              circle\r\n            ></el-button>\r\n            <el-button type=\"warning\" @click=\"toOrderDetail(scope.row.wait)\" icon=\"el-icon-tickets\" circle ></el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pageIndex\"\r\n        :page-sizes=\"[5, 10]\"\r\n        :page-size=\"queryInfo.pageSize\"\r\n        layout=\"total, sizes, prev, pager, next\"\r\n        :total=\"total\"\r\n      >\r\n      </el-pagination>\r\n    </el-card> \r\n    <el-dialog title=\"修改订餐信息\" :visible.sync=\"editOrderShow\" width=\"80%\" center @close=\"editOrderShow=false\" >\r\n      <!-- 内容主题区域 -->\r\n      <el-table :data=\"ors\" border stripe>\r\n        <el-table-column type=\"index\"></el-table-column>\r\n        <el-table-column label=\"食品名称\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            {{scope.row.card.food.fname}}\r\n          </template>\r\n        </el-table-column>\r\n         <el-table-column label=\"购买份数\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n            {{scope.row.card.num}}\r\n          </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"订餐状态\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tag v-if=\"scope.row.wstate==0\" type=\"warning\">备餐中</el-tag>\r\n              <el-tag v-if=\"scope.row.wstate==1\" type=\"danger\">待取餐</el-tag>\r\n              <el-tag v-if=\"scope.row.wstate==2\" type=\"success\">已完成</el-tag>\r\n          </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"购买时间\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n            {{scope.row.otime | timeflow}}\r\n          </template>\r\n         </el-table-column>\r\n          <el-table-column label=\"操作\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-select @change=\"changeItem(scope.row)\" v-model=\"scope.row.wstate\">\r\n              <el-option\r\n                v-for=\"item in states\"\r\n                :key=\"item.ostate\"\r\n                :label=\"item.value\"\r\n                :value=\"item.ostate\">\r\n              </el-option>\r\n            </el-select>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 底部区域 -->\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-radio-group @change=\"changes()\" v-model=\"radio\">\r\n          <el-radio :label=\"0\">备餐中</el-radio>\r\n          <el-radio :label=\"1\">待取餐</el-radio>\r\n          <el-radio :label=\"2\">已完成</el-radio>\r\n        </el-radio-group>\r\n      </span>\r\n    </el-dialog>\r\n\r\n\r\n     <el-dialog title=\"订餐整体信息\" :visible.sync=\"orderDetailShow\" width=\"80%\" center @close=\"orderDetailShow=false\" >\r\n      <!-- 内容主题区域 -->\r\n      <el-table :data=\"ors\" border stripe>\r\n        <el-table-column type=\"index\"></el-table-column>\r\n        <el-table-column label=\"食品名称\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            {{scope.row.card.food.fname}}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"食品单价\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            {{scope.row.card.food.fprice.toFixed(2)}}\r\n          </template>\r\n        </el-table-column>\r\n         <el-table-column label=\"购买份数\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n            {{scope.row.card.num}}\r\n          </template>\r\n         </el-table-column>\r\n          <el-table-column label=\"总价\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            {{scope.row.card.tprice.toFixed(2)}}\r\n          </template>\r\n        </el-table-column>\r\n         <el-table-column label=\"订餐状态\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tag v-if=\"scope.row.wstate==0\" type=\"warning\">备餐中</el-tag>\r\n              <el-tag v-if=\"scope.row.wstate==1\" type=\"danger\">待取餐</el-tag>\r\n              <el-tag v-if=\"scope.row.wstate==2\" type=\"success\">已完成</el-tag>\r\n          </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"购买人称呼\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n            {{scope.row.user.nike}}\r\n          </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"购买人电话\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n            {{scope.row.user.phone}}\r\n          </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"购买时间\" align=\"center\">\r\n            <template slot-scope=\"scope\">\r\n            {{scope.row.otime | timeflow}}\r\n          </template>\r\n         </el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from 'axios'\r\nimport Vue from 'vue'\r\n\r\n//过滤器，对时间格式进行转换\r\nVue.filter('timeflow',date=>{\r\n  if(!date) return ''\r\n  const da = new Date(date);\r\n  let time = da.getFullYear();\r\n  const month = da.getMonth()+1;\r\n  const day = da.getDate();\r\n  //针对月份，天进行拼接处理\r\n  if(month<10)time=time+'-0'+month;\r\n  else time = time+'-'+month;\r\n  if(day<10)time=time+'-0'+day;\r\n  else time = time+'-'+day;\r\n  return time;\r\n})\r\nexport default {\r\n   data() {\r\n    return {\r\n      loading:false,\r\n      orders:[],\r\n      ors:[],\r\n      total:0,\r\n      radio: 0,\r\n      orderWait:'',\r\n      editOrderShow:false,\r\n      orderDetailShow:false,\r\n      checkedState:'',\r\n      editOrderInfo:{\r\n        ostate:'',\r\n      },\r\n      states:[\r\n        {ostate:'0',value:'备餐中'},\r\n        {ostate:'1',value:'待取餐'},\r\n        {ostate:'2',value:'已完成'}\r\n      ],\r\n      queryInfo:{\r\n        query:'',\r\n        pageIndex:1,\r\n        pageSize:5\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    //获取所有订单信息\r\n    getOrders(){\r\n      axios.post('/api/dincan/admin/getOrdersPa',this.queryInfo).then(res=>{\r\n        this.orders = res.data.list;\r\n        this.total = res.data.count;\r\n      })\r\n    },\r\n    handleSizeChange(val){\r\n      this.queryInfo.pageSize = val;\r\n      this.getOrders()\r\n    },\r\n    handleCurrentChange(val){\r\n      this.queryInfo.pageIndex = val;\r\n      this.getOrders()\r\n    },\r\n    search(){\r\n      this.getOrders()\r\n    },\r\n    toEdit(wait,wstate){\r\n      axios.get('/api/dincan/admin/getOrders?wait='+wait).then(res=>{\r\n        this.orderWait = wait;\r\n        this.radio = parseInt(wstate);\r\n        this.ors = res.data;\r\n        this.editOrderShow = true;\r\n      })\r\n      \r\n    },\r\n    changeItem(or){\r\n      axios.get('/api/dincan/admin/updateOrder?oid='+or.oid+'&wstate='+or.wstate).then(res=>{\r\n        if(res.data==1){\r\n          this.getOrders();\r\n        }else{\r\n          this.$message.error('修改失败，请重新尝试！');\r\n        }\r\n      })\r\n    },\r\n    changes(){\r\n      console.log(this.orderWait,this.radio)\r\n      axios.get('/api/dincan/admin/updateOrders?wait='+this.orderWait+'&wstate='+this.radio).then(res=>{\r\n        if(res.data>0){\r\n          this.getOrders();\r\n          this.editOrderShow = false;\r\n        }else{\r\n          this.$message.error('修改失败，请重新尝试！');\r\n        }\r\n      })\r\n    },\r\n    toOrderDetail(wait){\r\n       axios.get('/api/dincan/admin/getOrders?wait='+wait).then(res=>{\r\n        this.ors = res.data;\r\n        this.orderDetailShow = true;\r\n      })\r\n    }\r\n  },\r\n  mounted(){\r\n    this.getOrders();\r\n  },\r\n}\r\n</script>\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components/din"}]}