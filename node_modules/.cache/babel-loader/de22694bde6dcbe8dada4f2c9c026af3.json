AAA;AACA,IAAA,KADA,mBACA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,2BAAA,EAAA,KAAA,QAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA,EADA,CAEA;;;AACA,UAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,OAAA,EAAA,KAAA,CAAA,QAAA,CAAA,SAAA;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA,SALA,MAKA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA,GAAA,CAAA,IAAA,CAAA,GAAA,GAAA,GAAA;AACA;AACA,OAVA;AAYA;AAfA;AApCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"login_bg\"\r\n      v-loading=\"loading\"\r\n      element-loading-text=\"正在登录中,请稍等！\"\r\n      element-loading-spinner=\"el-icon-loading\"\r\n      element-loading-background=\"rgba(0, 0, 0, 0.8)\">\r\n    <div class=\"block\">\r\n      <el-carousel height=\"500px\">\r\n        <el-carousel-item v-for=\"item in imgs\" :key=\"item\">\r\n          <img :src=\"'/api'+item\" alt=\"\">\r\n        </el-carousel-item>\r\n      </el-carousel>\r\n    </div>\r\n    <div class=\"login_box\">\r\n      <div class=\"login-title\">\r\n        <h2>管理员登录</h2>\r\n      </div>\r\n      <!-- 登录表单 -->\r\n      <el-form\r\n        class=\"login_form\"\r\n        :model=\"ruleForm\"\r\n        :rules=\"rules\"\r\n        ref=\"form\"\r\n        label-width=\"0\"\r\n      >\r\n        <el-form-item prop=\"name\">\r\n          <el-input v-model=\"ruleForm.adminname\" placeholder=\"请输入用户名\">\r\n            <i slot=\"prefix\" class=\"fa fa-user\"></i>\r\n          </el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"password\">\r\n          <el-input v-model=\"ruleForm.password\" placeholder=\"请输入密码\">\r\n            <i slot=\"prefix\" class=\"fa fa-lock\"></i>\r\n          </el-input>\r\n        </el-form-item>\r\n        <el-button type=\"primary\" @click=\"login\">登录</el-button>\r\n        <!-- <a href=\"#\">注册</a> -->\r\n        <!-- <el-button type=\"info\">重置</el-button> -->\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  data() {\r\n    return {\r\n      loading:false,\r\n      imgs:[\r\n        \"/img/show/1.jpg\",\r\n        \"/img/show/2.jpg\",\r\n        \"/img/show/3.jpg\",\r\n        \"/img/show/4.jpg\",\r\n      ],\r\n      ruleForm: {\r\n        adminname: 'admin',\r\n        password: '123456',\r\n      },\r\n      rules: {\r\n        name: [\r\n          { required: true, message: '请输入用户名', trigger: 'blur' },\r\n          {\r\n            min: 3,\r\n            max: 10,\r\n            message: '长度在 3 到 10 个字符',\r\n            trigger: 'blur',\r\n          },\r\n        ],\r\n        password: [\r\n          { required: true, message: '请输入密码', trigger: 'blur' },\r\n          {\r\n            min: 6,\r\n            max: 15,\r\n            message: '长度在 6 到 15 个字符',\r\n            trigger: 'blur',\r\n          },\r\n        ],\r\n      },\r\n    }\r\n  },\r\n  methods: {\r\n    login(){\r\n      this.loading = true;\r\n      axios.post('/api/ssm/admin/checkAdmin',this.ruleForm).then(res=>{\r\n          this.loading = false;\r\n          if(res.data.state){\r\n            this.$message.success('登陆成功！');\r\n            //  token只应在当前网站打开期间生效，所以将token保存在sessionStorage中\r\n            window.sessionStorage.setItem('token', this.ruleForm.adminname);\r\n             this.$router.push('/home');\r\n          }else{\r\n             this.$message.error('登录失败,'+res.data.msg+\"!\");\r\n          }\r\n      })\r\n       \r\n    }\r\n  },\r\n}\r\n</script>\r\n<style lang=\"less\" scoped>\r\n.login_bg {\r\n  height: 100%;\r\n  background-image: url('/api/img/show/bg1.png');\r\n}\r\n.block{\r\n  width: 350px;\r\n  height: 500px;\r\n  position: absolute;\r\n  left: 50%;\r\n  top: 50%;\r\n  background: #fff;\r\n  transform: translate(-90%,-50%);\r\n   img{\r\n    width: 100%;\r\n    height: 100%;\r\n  }\r\n}\r\n\r\n.login_box {\r\n  width: 350px;\r\n  height: 500px;\r\n  background-color: rgb(248, 246, 246);\r\n  position: absolute;\r\n  left: 50%;\r\n  top: 50%;\r\n  transform: translate(10%, -50%);\r\n  // border-left: 1px solid rgb(25, 186, 235);\r\n}\r\n.login-title{\r\n  position: absolute;\r\n  top: 10%;\r\n  width: 70%;\r\n  left: 50%;\r\n  transform: translate(-50%,20%);\r\n}\r\n.login_form {\r\n  position: absolute;\r\n  width: 70%;\r\n  left: 50%;\r\n  transform: translate(-50%, 100%);\r\n}\r\n.login_form > .el-input {\r\n  width: 100%;\r\n  padding: 20px;\r\n}\r\n.login_form > .el-button {\r\n  width: 100%;\r\n  margin-left: 50%;\r\n  transform: translate(-50%);\r\n}\r\n.fa {\r\n  /* margin:0 15px; */\r\n  margin-left: 7px;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}