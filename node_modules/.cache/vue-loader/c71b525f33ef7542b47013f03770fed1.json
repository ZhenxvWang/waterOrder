  <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item>Home page</el-breadcrumb-item>\r\n      <el-breadcrumb-item>User management</el-breadcrumb-item>\r\n      <el-breadcrumb-item>List of users</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!-- 卡片视图区域 -->\r\n    <el-card>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"5\">\r\n          <el-input placeholder=\"Please enter your name\" v-model=\"queryInfo.query\" clearable></el-input>\r\n        </el-col>\r\n        <el-col :span=\"3\">\r\n          <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"getList\">search</el-button>\r\n        </el-col>\r\n      </el-row>\r\n      <!-- 用户列表区 -->\r\n      <el-table  v-loading=\"loading\" :data=\"users\" border stripe>\r\n        <el-table-column type=\"index\"></el-table-column>\r\n        <el-table-column label=\"user name\" prop=\"username\"> </el-table-column>\r\n        <el-table-column label=\"nickname\" prop=\"nike\"> </el-table-column>\r\n        <el-table-column label=\"phone\" prop=\"phone\"> </el-table-column>\r\n         <el-table-column label=\"account balance\" prop=\"acount\"> </el-table-column>\r\n        <el-table-column label=\"operation\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              @click=\"toEdit(scope.row)\"\r\n              type=\"primary\"\r\n              icon=\"el-icon-edit\"\r\n              circle\r\n            ></el-button>\r\n            <el-button @click=\"userDelete(scope.row.uid)\"\r\n             type=\"danger\" icon=\"el-icon-delete\" circle ></el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[5, 10]\"\r\n        :page-size=\"queryInfo.pageSize\"\r\n        layout=\"total, sizes, prev, pager, next\"\r\n        :total=\"total\"\r\n      >\r\n      </el-pagination>\r\n    </el-card>\r\n    <el-dialog title=\"Modify the user\" :visible.sync=\"editShow\" width=\"50%\" center @close=\"editClose\" >\r\n      <!-- 内容主题区域 -->\r\n      <el-form :model=\"editUser\" :rules=\"editRules\" ref=\"editRules\" label-width=\"80px\">\r\n        <el-form-item label=\"nickname\" prop=\"nike\">\r\n          <el-input v-model=\"editUser.nike\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"password\" prop=\"password\">\r\n          <el-input v-model=\"editUser.password\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"phone\" prop=\"phone\">\r\n          <el-input v-model=\"editUser.phone\"></el-input>\r\n        </el-form-item>\r\n         <el-form-item label=\"account balance\" prop=\"acount\">\r\n          <el-input v-model=\"editUser.acount\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n      <!-- 底部区域 -->\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editClose()\">cancel</el-button>\r\n        <el-button type=\"primary\" @click=\"editCommit('editRules')\">Confirm the change</el-button>\r\n      </span>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  data() {\r\n    // 定义电话校验规则\r\n    var checkMobie = (rule,value,callback)=>{\r\n      const regMobie = /^((0\\d{2,3}-\\d{7,8})|(1[3456789]\\d{9}))$/\r\n      if(value==''){callback(new Error('Please enter your mobile phone number'));}\r\n      else if(!regMobie.test(value)){callback(new Error('Please enter a valid mobile phone number'));}\r\n      else callback();\r\n    }\r\n\r\n    var checkPassword =  (rule,value,callback)=>{\r\n      const regPa = /^\\w{6,16}$/\r\n      if(value=='')callback(new Error('Please enter your password'));\r\n      else if(!regPa.test(value))callback(new Error('The password contains 6 to 16 digits and underscores (_)'));\r\n      else callback();\r\n    }\r\n\r\n    var checkUsername =  (rule,value,callback)=>{\r\n      const regPa = /^\\w{4,16}$/\r\n      if(value=='')callback(new Error('Please enter a user name'));\r\n      else if(!regPa.test(value)){\r\n        callback(new Error('The user name contains 4 to 16 digits and underscores (_)'));\r\n      }else{\r\n        axios.post('/api/ssm/user/checkUserRegist',{'username':value}).then(res=>{\r\n          if(res.data.flag){\r\n            callback(new Error('User name already exists!'));\r\n          }else{\r\n            callback();\r\n          }\r\n        })\r\n      }\r\n    }\r\n\r\n\r\n    return {\r\n      total: 0,\r\n      loading:true,\r\n      queryInfo: {\r\n        query: '',\r\n        pagenum: 1,\r\n        pageSize: 5,\r\n      },\r\n      users:[],\r\n      editShow:false,\r\n      editUser:{\r\n        uid:'',\r\n        nike:'',\r\n        phone:'',\r\n        password:'',\r\n        acount:''\r\n      },\r\n      //表单的验证规则对象\r\n      editRules: {\r\n        nike: [{required: true,message: 'Please enter a nickname', trigger: 'blur',}],\r\n        username: [{required: true,validator:checkUsername, trigger: 'blur',}],\r\n        phone: [{required: true,validator:checkMobie,trigger:'blur'}],\r\n        password:[{required: true,validator:checkPassword,trigger:'blur'}],\r\n      },\r\n    }\r\n  },\r\n  methods: {\r\n    //获取所有的用户信息\r\n    getList() {\r\n      axios.post('/api/order/admin/getUsers',this.queryInfo).then(res=>{\r\n        this.loading = false;\r\n        this.total = res.data.count;\r\n        this.users = res.data.list;\r\n      })\r\n    },\r\n    //底部每页大小改变触发\r\n    handleSizeChange(newSize) {\r\n      this.queryInfo.pageSize = newSize\r\n      this.getList()\r\n    },\r\n    //底部每页当前页码改变时触发\r\n    handleCurrentChange(newPage) {\r\n      this.queryInfo.pagenum = newPage\r\n      this.getList()\r\n    },\r\n    toEdit(user){\r\n      this.editUser.uid = user.uid;\r\n      this.editUser.nike = user.nike;\r\n      this.editUser.password = user.password;\r\n      this.editUser.phone = user.phone;\r\n      this.editUser.acount = user.acount;\r\n      \r\n      this.editShow = true;\r\n    },\r\n    editClose(){\r\n      this.editShow = false;\r\n      this.$nextTick(()=>{\r\n        this.$refs.editRules.resetFields();\r\n      });\r\n    },\r\n    editCommit(formName){\r\n      this.$refs[formName].validate(valid => {\r\n        if(valid){\r\n          //请求\r\n          axios.post('/api/order/admin/updateUserBase',this.editUser).then(res=>{\r\n            if(res.data==1){\r\n              this.$message.success('Modified successfully!');\r\n              this.getList();\r\n            }else{\r\n              this.$message.error('Modification failed!');\r\n            }\r\n            this.editShow = false;\r\n          })\r\n        \r\n        }else{\r\n          return false;\r\n        }\r\n      })\r\n\r\n    },\r\n    async userDelete(id) {\r\n      const re = await this.$confirm('This operation will permanently delete the user. Do you want to continue?', 'prompt', {\r\n          confirmButtonText: 'YES',\r\n          cancelButtonText: 'NO',\r\n          type: 'warning'\r\n      }).catch(err=>err)\r\n      // console.log(re)\r\n      if(re !='confirm'){\r\n        this.$message.info('Cancelled deletion!')\r\n      }else{\r\n        axios.get('/api/order/admin/deleteUser?uid='+id).then(res=>{\r\n          if(res.data==1){\r\n            this.$message.success('Deleted successfully!')\r\n            this.getList();\r\n          }else{\r\n            this.$message.error('Delete failed!')\r\n          }\r\n        })\r\n      }\r\n    },\r\n  \r\n  },\r\n  mounted() {\r\n    this.getList();\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n  body {\r\n    margin: 0;\r\n  }\r\n</style>"]}]}